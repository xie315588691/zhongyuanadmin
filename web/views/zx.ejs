<body>

<div id="warpper">

    <%- include component/header.ejs %>
    <section>
        <div class="contain">
            <div class="nybanner"
                 style="background-image:url(http://image.xiaoxinseo.com/8e3202ada23282161affdf8e61446b4c.jpg);">

                <div class="ny-title">
                    <h2 class="wow bounceInDown"><img src="images/2017111617503466.png"/></h2>
                    <h3 class="wow fadeInUp"><img src="images/2017111617503476.png"/></h3></div>
            </div>
            <div class="navigation">
                <div class="layout">
                    <a class="home" href="index.html">首页</a>
                    <em>></em>
                    <a class="on" href="zx.html">新闻资讯</a>
                </div>
            </div>
            <div class="news-warp">
                <div class="layout">
                    <!--                    <div class="news-screen clearfix">-->
                    <!--                        <div class="screenleft">-->
                    <!--                            <ul>-->
                    <!--                                <li><a href="./fangAn.html">税筹方案</a></li>-->
                    <!--                                <li><a href="xwinfo.html#">行业干货</a></li>-->
