                    <!--                            </ul>-->
                    <!--                        </div>-->
                </div>

                <div class="newslist">
                    <div class="hotbom clearfix" id="ul">
                        <div class="item" v-for="item in articles" :key="item.id">
                            <a :href="'/news/'+item.id">
                                <div class="tmain">
                                    <span class="time">{{ item.time}}</span>
                                    <h3 class="title overflowtxt">{{item.title}}</h3>
                                    <div class="h3">
                                        {{item.descript}}
                                    </div>
                                    <span class="more">Load More</span>
                                </div>
                            </a>
                        </div>

                    </div>
                </div>

                <div class="pageinfo" style="line-height:40px;">
                    <div class="block">
                        <el-pagination
                                @size-change="handleSizeChange"
                                @current-change="handleCurrentChange"
                                :current-page="currentPage"
                                :page-sizes="[16, 20, 40]"
                                :page-size="pageSize"
                                layout="total, sizes, prev, pager, next, jumper"
                                :total="totalPage">
                        </el-pagination>
                    </div>
                </div>
            </div>
        </div>
        <div class="gf-demand">
            <div class="layout">
                <h2 class="h2">I NEED TO BUILD WEBSITE</h2>
                <h3 class="h3">获取免费税筹方案</h3>
                <form action="#" method="post"
                      onSubmit="return jianzhan()">
                    <div class="write">
                        <input class="text1" type="text" id="name" name="name" placeholder="称呼姓名" value="">
                        <input class="text2" type="text" id="phone" name="phone" placeholder="联系电话" value="">
                        <input class="text3" type="text" id="content" name="content"
                               placeholder="企业名称" value="">
                        <input type="hidden" name="pid" value="1"/>
                        <input class="submit" type="submit" value="领取免费税筹方案">
                    </div>
                </form>
                <i class="italic">*请认真填写需求信息，我们会在24小时内与您取得联系。</i>
            </div>
        </div>
